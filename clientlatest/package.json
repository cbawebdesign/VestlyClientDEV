{
  "name": "vestly",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "setAndroidDevelopment": "ENVFILE=.env",
    "android": "yarn setAndroidDevelopment react-native run-android",
    "setDevelopment": "ENVFILE=.env.development",
    "setStaging": "ENVFILE=.env.staging",
    "setProduction": "ENVFILE=.env.production",
    "ios:dev": "yarn setDevelopment react-native run-ios --mode=Debug --scheme 'VestlyDevelopment'",
    "ios:staging": "yarn setStaging react-native run-ios --mode=Debug --scheme 'VestlyStaging'",
    "ios:prod": "yarn setProduction react-native run-ios --mode=Debug --scheme 'Vestly'",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "clean": "rm -rf ${HOME}/Library/Developer/Xcode/DerivedData/*"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4",
    "@react-native-firebase/app": "^18.5.0",
